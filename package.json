{
  "name": "api-node-template",
  "version": "1.1.0",
  "description": "This is a template for backend development in IPD",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/InterfazProductosDigitales/api-node-template.git"
  },
  "scripts": {
    "faker": "babel-node ./src/factory/clients",
    "init:prod": "cross-env NODE_CONFIG_ENV=production node ./dist/scripts/initial-setup",
    "init:dev": "babel-node ./src/scripts/initial-setup",
    "build": "rimraf dist && cross-env BABEL_ENV=production babel src --out-dir dist",
    "nodemon": "nodemon --exec npx babel-node ./src/server",
    "dev": "cross-env NODE_CONFIG_ENV=development && npm run init:dev && npm run nodemon",
    "start": "cross-env NODE_CONFIG_ENV=production node ./dist/server",
    "lint": "npx eslint ./src --fix",
    "test": "cross-env NODE_CONFIG_ENV=test jest --verbose --watchAll --detectOpenHandles",
    "component": "cross-var ./src/components/$npm_config_component",
    "test:component": "cross-env NODE_CONFIG_ENV=test jest ./src/components/%npm_config_component% --verbose --watchAll --detectOpenHandles"
  },
  "keywords": [],
  "author": "IPD development team",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "7.17.0",
    "@babel/core": "7.17.2",
    "@babel/node": "7.16.8",
    "@babel/preset-env": "7.16.11",
    "@google-cloud/storage": "5.18.3",
    "bcrypt": "5.0.1",
    "config": "3.3.7",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "cross-var": "^1.1.0",
    "dayjs": "1.11.0",
    "dotenv": "16.0.0",
    "express": "4.17.2",
    "express-validator": "^6.14.1",
    "googleapis": "^39.2.0",
    "helmet": "5.0.2",
    "http-errors": "2.0.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "6.2.1",
    "mongoose-aggregate-paginate-v2": "^1.0.6",
    "mongoose-delete": "0.5.4",
    "mongoose-paginate-v2": "1.6.1",
    "morgan": "1.10.0",
    "multer": "1.4.4",
    "nanoid": "3.3.1",
    "passport": "0.5.2",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "rimraf": "3.0.2",
    "sequelize": "6.17.0",
    "uuid": "8.3.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@faker-js/faker": "7.1.0",
    "babel-jest": "27.5.1",
    "babel-node": "0.0.1-security",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-preset-env": "1.7.0",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-module-resolver": "1.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "supertest": "6.2.2"
  }
}
